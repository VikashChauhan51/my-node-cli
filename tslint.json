{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "semicolon": false,
      "trailing-comma": false,
      "no-duplicate-variable": true,
      "no-empty-interface": true,
      "no-eval": true,
      "no-shadowed-variable": true,
      "no-var-keyword": true
    }
  }